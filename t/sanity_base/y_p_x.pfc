:- use_module(library(pfc)).

ttt(a,p,b).
ttt(b,p,c). 
ttt(c,p,d).
ttt(d,p,c). 
ttt(c,p,b).
ttt(b,p,a).

:- mpred_trace_exec.

ttt(Z,p,Y),ttt(Y,p,X) ==> ttt(X,p,Z).

:- printAll(ttt(_What,p,a)).

